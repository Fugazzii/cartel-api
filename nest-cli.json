{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/cocaines/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/cocaines/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/cartel-api",
  "projects": {
    "members": {
      "type": "application",
      "root": "apps/members",
      "entryFile": "main",
      "sourceRoot": "apps/members/src",
      "compilerOptions": {
        "tsConfigPath": "apps/members/tsconfig.app.json"
      }
    },
    "cocaines": {
      "type": "application",
      "root": "apps/cocaines",
      "entryFile": "main",
      "sourceRoot": "apps/cocaines/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cocaines/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "nats": {
      "type": "library",
      "root": "libs/nats",
      "entryFile": "index",
      "sourceRoot": "libs/nats/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nats/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    }
  }
}